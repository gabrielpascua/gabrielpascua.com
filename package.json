{
  "name": "i-am-gabriel-pascua",
  "description": "Personal site of Gabriel Pascua",
  "version": "6.0.0",
  "author": "Gabriel Pascua <mail@gabrielpascua.com>",
  "dependencies": {
    "gatsby": "^2.23.1",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-remark-prismjs": "^3.5.3",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-json": "^2.4.4",
    "gatsby-transformer-remark": "^2.8.14",
    "moment": "^2.26.0",
    "prismjs": "^1.20.0",
    "purecss": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "eslint . && gatsby build && gatsby develop",
    "deploy": "gatsby clean && gatsby build --prefix-paths --verbose",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "eslint . && gatsby serve",
    "clean": "gatsby clean",
    "upgrade-interactive": "npm-check --update"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielpascua/gabrielpascua.github.io"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write \"**/*.{js,jsx}\""
    ]
  }
}
