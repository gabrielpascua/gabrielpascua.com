{
  "name": "i-am-gabriel-pascua",
  "description": "Personal site of Gabriel Pascua",
  "version": "5.0.0",
  "author": "Gabriel Pascua <mail@gabrielpascua.com>",
  "dependencies": {
    "gatsby": "^2.0.0",
    "gatsby-paginate": "^1.0.16",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-json": "^2.1.4",
    "gatsby-transformer-remark": "^2.1.6",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "prismjs": "^1.15.0",
    "purecss": "^1.0.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "eslint . && gatsby develop",
    "deploy": "gatsby build --prefix-links",
    "format": "prettier --write '**/*.js'",
    "serve": "eslint . && gatsby serve"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielpascua/gabrielpascua.github.io"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write '**/*.js'",
      "git add"
    ]
  }
}